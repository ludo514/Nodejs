<%- include("header") -%> 
        <div class="box" id="heading">
            <h1><%= ListTitle %> !</h1>
        </div>
        <div class="box">
            <% for(i=0; i < newItem.length; i++){%>
                <div class="item">
                    <input type="checkbox">
                    <p><%= newItem[i] %> </p>
                </div>
            <% } %>

            <form action="/" method="POST">
                <input type="text" name="add" placeholder="New item" autocomplete="off">
                <button type="submit" name="list" value=<%= ListTitle %> >+</button>
            </form>
        </div>
<%- include("footer") -%> 